generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id     String     @id @default(uuid())
    name   String
    castles Castle[]
}

model Castle {
    id     String   @id @default(uuid())
    x      Int
    y      Int
    user   User     @relation(fields: [userId], references: [id])
    userId String   @unique
}
